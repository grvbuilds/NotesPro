{
  "manifest_version": 3,
  "name": "NotesX",
  "version": "1.0",
  "description": "A pro note-taking Chrome extension",
  "permissions": ["storage"],
  "action": {
    "default_popup": "popup/index.html",
    "default_icon": {
      "16": "images/icon16.png",
      "48": "images/icon48.png",
      "128": "images/icon128.png"
    }
  },
  "icons": {
    "16": "images/icon16.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  },
  "manifest_version": 3
}
